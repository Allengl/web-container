<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>queryRoomSevlet</servlet-name>
    <servlet-class>com.gl.hotel.QueryRoomServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>queryRoomSevlet</servlet-name>
    <url-pattern>/queryroom</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>bookRoomSevlet</servlet-name>
    <servlet-class>com.gl.hotel.BookRoomServlet</servlet-class>
<!--    <load-on-startup>0</load-on-startup>-->
  </servlet>
  <servlet-mapping>
    <servlet-name>bookRoomSevlet</servlet-name>
    <url-pattern>/bookroom</url-pattern>
  </servlet-mapping>



</web-app>
